   "source": [
    "def calchar(s):\n",
    "    ucnt=0\n",
    "    lcnt=0\n",
    "    for i in s:\n",
    "        if i.isupper():\n",
    "            ucnt+=1\n",
    "        elif i.islower():\n",
    "            lcnt+=1\n",
    "    return ucnt,lcnt\n",
    "\n",
    "a=calchar(input(\"Enter string\"))\n",
    "print(a)\n",
    "print(\"Upper characters=\",a[0])\n",
    "print(\"lower characters=\",a[1])"
   ]
